package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	private int row;
	private char cols;
    @Override
    public int getRow()
    {
        return this.row;
    }

    @Override
    public int getCol()
    {
        return this.cols;
    }
    
    
    public SpreadsheetLocation(String cellName)
    {
    	char cols= cellName.charAt(0);
    	int row= cellName.charAt(1)+cellName.charAt(2); 
    }

}
