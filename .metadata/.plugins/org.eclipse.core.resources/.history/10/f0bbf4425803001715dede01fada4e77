package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	private int row;
	private int cols;
	private String alphabet = "ABCDEFJHIJKLMNOPQRSTUVWXYZ";

    @Override
    public int getRow()
    {
        return row-1;
    }

    @Override
    public int getCol()
    {
        return cols;
    }
    
    
    public SpreadsheetLocation(String cellName)
    {
   	 char letter= cellName.charAt(0);

    	 for (int i =0; i <28;i++){
    		 if  (cellName.charAt(0) == alphabet.charAt(i)){
    			 cols=i; 
    		 }
    	 }
    	 int row= Integer.parseInt(cellName.substring(1))-1; 
    }

}
