	public static double absValue(double a){
		if (a>0){
			return(a);
		}
		else return(a*-1);
	}
		
	public static int max(int a, int b){
		if (a>b){
			return(a);
		}
		else return(b);
	}
	
	public static double max(double a, double b, double c){
		if (a>b a>c){
			return(a);
		}
		if (b>a b>c){
			return(b);
		}
		else return(c);
	}
	
	public static int min(int a, int b){
		if (a<b){
			return(a);
		}
		else return(b);
	}
	
	public static double round2(double a){
		return(a*100/100.0);
	}
	
