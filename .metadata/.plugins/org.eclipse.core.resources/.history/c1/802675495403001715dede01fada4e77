package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	private int row;
	private char cols;
    @Override
    public int getRow()
    {
        return row;
    }

    @Override
    public int getCol()
    {
        return cols;
    }
    
    
    public SpreadsheetLocation(String cellName)
    {
    	 cols= cellName.charAt(0);
    	 row= cellName.charAt(1)+cellName.charAt(2); 
    }

}
